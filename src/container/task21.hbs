<br>
{{#with heading}}
<div class="card">
    <div class="card-body">

        {{#with main}}
        <div class="card">
            <div class="card-body">
                <h1>{{title}}</h1>
                <p>{{text}}</p>
                <button type="button" class="btn btn-primary">{{button_text.solid}}</button>
                <br>
                <br>
                <button type="button" class="btn btn-outline-primary">{{button_text.outline}}</button>
            </div>
        </div>
        {{/with}}

        <br>

        {{#with second}}
        <div class="card">
            <div class="card-body">
                <h2>{{title}}</h2>
                <p>{{text}}</p>
                <button type="button" class="btn btn-secondary">{{button_text.solid}}</button>
                <br>
                <br>
                <button type="button" class="btn btn-outline-secondary">{{button_text.outline}}</button>
            </div>
        </div>
        {{/with}}

        <br>

        {{#with subblock}}
        <div class="card">
            <div class="card-body">
                <h3>{{title}}</h3>
                <p>{{text}}</p>
                <button type="button" class="btn btn-dark">{{button_text.solid}}</button>
                <br>
                <br>
                <button type="button" class="btn btn-outline-dark">{{button_text.outline}}</button>
            </div>
        </div>
        {{/with}}

    </div>
</div>
{{/with}}

<br>

{{#with nested_display}}
<div class="card">
    <div class="card-body">
        <div class="display-1">{{title}}</div>

        {{#with children_display}}
        <div class="card">
            <div class="card-body">
                <div class="display-4">{{title}}</div>
                <p>{{text}}</p>
                <button type="button" class="btn btn-outline-warning">{{button.text}}</button>
            </div>
        </div>
        {{/with}}

        <br>

        <h6>{{header_text}}</h6>
        <button type="button" class="btn btn-info text-light">{{button.text}}</button>
    </div>
</div>
{{/with}}

<br>

{{#with paragraph}}
<div class="card">
    <div class="card-body">

        <p class="text-uppercase">{{title}}</p>
        <p class="text-lowercase">{{text}}</p>
        <br>
        {{#with button}}
        <div>
            <a class="btn text-info" href="{{url}}" role="button">{{text}}</a>
        </div>
        {{/with}}


        <div class="card">
            <div class="card_body">
                <ul>
                    {{#each list_name}}
                    <li>
                        <p>
                            <a class="btn text-info" href="{{url}}" role="button">{{text}}</a>
                        </p>
                    </li>
                    {{/each}}
                </ul>
            </div>
        </div>

    </div>
</div>
{{/with}}

<br>