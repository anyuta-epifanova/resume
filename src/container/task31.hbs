<div class="row gy-4">

    <section class="col-10">
        {{#with navigation}}
        <nav>
            {{#each links}}
            <a class="col-2 btn btn-link" href="{{href}}">{{text}}</a>
            {{/each}}
            <a class="col-2 btn btn-primary" href="{{button.href}}">{{button.text}}</a>
        </nav>
        {{/with}}
    </section>

    <hr>

    {{#with header}}
    <section class="col-12">
        <header>
            <h1>{{title}}</h1>
            <p>{{description}}</p>
            <a class="col-2 btn btn-primary" href="{{button.link}}">{{button.text}}</a>
        </header>
    </section>
    {{/with}}

    <hr>

    {{#with city}}
    <section class="col-12">

        <h2>{{title}}</h2>

        <div class="row gx-2 gy-3">
            {{#each cards}}
            <div class="col-5">
                <div class="card">
                    {{#with picture}}
                    <img class="card-img-top" src="{{src}}" alt="{{alt}}">
                    {{/with}}
                    <div class="card-body">
                        <h5 class="card-title">{{title}}</h5>
                        <p>{{description}}</p>
                        {{#with button}}
                        <a href="{{href}}" class="btn btn-primary">{{text}}</a>
                        {{/with}}
                    </div>
                </div>
            </div>
            {{/each}}
        </div>

    </section>
    {{/with}}

    <hr>

    {{#with winner}}
    <section>
        <h2>{{title}}</h2>

        {{#with table}}
        <table class="table">
            <thead>
                <tr>
                    {{#each head}}
                    <th>
                        {{this}}
                    </th>
                    {{/each}}
                </tr>
            </thead>

            <tbody>
                {{#each user}}
                <tr>
                    <th>{{place}}</th>
                    <td>{{name}}</td>
                    <td>{{surname}}</td>
                    <td>@{{nickname}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        {{/with}}
    </section>
    {{/with}}

    <hr>

    {{#with gallery}}
    <section class="col-12">
        <h2>{{title}}</h2>

        <div class="row g-2">
            {{#each cards}}
            <div class="col-4">
                <div class="card">
                    <div class="card-header">
                        {{name}}
                    </div>

                    <ul class="list-group">
                        {{#each items}}
                        <li class="list-group-item">
                            {{this}}
                        </li>
                        {{/each}}
                    </ul>
                </div>
            </div>
            {{/each}}
        </div>
    </section>
    {{/with}}

    <hr>

    {{#with relaxion}}
    <section class="col-12">
        <p class="display-3">{{title}}</p>

        <div class="row gy-3 gx-1">
            {{#each cards}}
            <div class="col-5">
                <div class="card text-bg-secondary">
                    <div class="card-header">
                        {{header}}
                    </div>
                    <div class="card-body">
                        <h5 class="card-title">{{title}}</h5>
                        <p class="card-text">{{description}}</p>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>

    </section>
    {{/with}}

    <hr>

    {{#with rest}}
    <section col-12>
        <div class="row gx-2">
            {{#each cards}}
            <div class="col-6">
                <div class="card text-center">
                    <div class="card-header">
                        {{header}}
                    </div>
                    <div class="card-body">
                        <h5 class="card-title">{{title}}</h5>
                        <p class="card-text">{{description}}</p>
                        <a href="#" class="btn btn-primary">{{button}}</a>
                    </div>
                    <div class="card-footer">
                        {{footer}}
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </section>
    {{/with}}

    <hr>

    {{#with proposition}}
    <section class="col-12">
        <p class="display-2">{{title}}</p>

        <div class="row gx-2">
            {{#each card}}
            <div class="col-5">
                <div class="card text-bg-warning text-light">
                    <div class="card-header">
                        {{header}}
                    </div>
                    <div class="card-body">
                        <h5 class="card-title">{{title}}</h5>
                        <p class="card-text">{{description}}</p>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </section>
    {{/with}}
</div>